{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/**.cs",
            "**.cs"
          ]
        }
      ],
      "src": "../Assets",
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "manual/index.md"
        ]
      },
      {
        "files": [
          "manual/**.md",
          "manual/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadata" : {
      "_appFooter": "<span>
                      <span class=\"far fa-copyright\"></span>
                      2018, Affective & Cognitive Institute</span>
                      <a href=\"https://affective-lab.org\" class=\"fas fa-home\"></a>
                      <a href=\"https://www.researchgate.net/lab/Affective-and-Cognitive-Institute-Affective-Lab-Oliver-Korn\" class=\"fab fa-researchgate\"></a>
                      <a href=\"https://www.linkedin.com/company/affective-cognitive-institute/\" class=\"fab fa-linkedin\"></a>
                      <a href=\"https://www.youtube.com/channel/UCYetO2MEXJ0KMVWyxLXMS1g\" class=\"fab fa-youtube\"></a>
                      <a href=\"https://github.com/AffectiveCognitiveInstitute\" class=\"fab fa-github\"></a>",
      "_appFaviconPath": "images/favicon.ico",
      "_appLogoPath" : "images/logo.svg"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/aci"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}